{
  "name": "nestjs-authentication-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "-----------------dev-----------------": "",
    "dev": "NODE_ENV=development nest start --watch",
    "dev:prisma:mig": "dotenv -e ./env/.development.env -- npx prisma migrate dev",
    "dev:prisma:push": "dotenv -e ./env/.env.development -- npx prisma db push",
    "dev:swag": "nestia swagger --config nestia-dev.config.ts && NODE_ENV=development pnpm dev",
    "-------------------------------------": "",
    "start": "NODE_ENV=production nest start",
    "gen": "prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@nestia/core": "^3.6.1",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^3.6.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "^5.16.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "install": "^0.13.0",
    "ioredis": "^5.4.1",
    "kysely": "^0.27.4",
    "prisma-extension-kysely": "^2.1.0",
    "prisma-kysely": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typia": "^6.4.3"
  },
  "devDependencies": {
    "@nestia/sdk": "^3.6.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "nestia": "^5.3.1",
    "prettier": "^2.3.2",
    "prisma": "^5.16.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
